<h1>Sign In</h1>
<%= form_tag(user_authenticate_path, :method => "POST") do %>
    <div class="form-group">
      <%= label_tag :email %>
      <%= email_field_tag :email, params[:email] %>
    </div>
    <div class="form-group">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class="form-group">
      <%= submit_tag "Sign-in"%>
    </div>
<% end %>
